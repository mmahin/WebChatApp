<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Signup</title>
</head>
<body>
<form>
<label for="fname">First Name:     </label>
<input id="fname" type="text" name="fname"><br><br>
<label for="lname">Last Name:      </label>
<input id="lname" type="text" name="lname"><br><br>
<label for="username">Username:       </label>
<input id="username" type="text" name="username"><br><br>
<label for="email">Email Id:       </label>
<input id="email" type="text" name="email"><br><br>
<label for="gender">Gender:         </label>
<input id="gender" type="text" name="gender"><br><br>
<label for="bday">Birthday:       </label>
<input id="bday" type="text" name="bday"><br><br>
<label for="country">Country:        </label>
<input id="country" type="text" name="country"><br><br>
<label for="password">Password:       </label>
<input id="password" type="password" name="password"><br><br>
<label for="repassword">Retype Password:</label>
<input id="repassword" type="password" name="repassword"><br><br>
<input type="submit" value="Signup">
</form>
</body>
</html>